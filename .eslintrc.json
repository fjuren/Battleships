{
    "env": {
        "browser": true,
        "es2021": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": 0,
        "indent":"off",
        "no-multiple-empty-lines":"off",
        "max-len":"off"
    }
}
